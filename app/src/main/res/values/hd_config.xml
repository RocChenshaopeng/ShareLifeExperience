    <style name="ProAuthorInfoNormalText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/profile_author_info_textview_minheight</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginStart">@dimen/profile_author_info_margen_left</item>
    </style>

    <style name="ProAuthorBaseInfoLinearTheme">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:minHeight">@dimen/profile_author_info_textview_minheight</item>
        <item name="android:paddingLeft">@dimen/profile_author_info_margen_left</item>
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="AppTheme.ActionBar">
        <item name="android:displayOptions">showHome|showTitle|homeAsUp</item>
        <item name="android:height">@dimen/actionbar_height</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:titleTextStyle">@style/DialtactsActionBarTitleText</item>
        <item name="android:contentInsetStart">@dimen/abc_action_bar_content_inset_material</item>
    </style>

    <style name="DialtactsActionBarTitleText"
        parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="TimeLine_Left_TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="ListView_Title_TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="ListView_Summary_TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="ListView_TextView_Subject">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/leshareThemeColor</item>
    </style>

    <style name="Subject_item_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/subject_item_textview_background</item>
        <item name="android:layout_marginLeft">@dimen/listview_item_margen_left</item>
        <item name="android:layout_marginTop">@dimen/listview_item_margen_top</item>
        <item name="android:textSize">@dimen/announce_text_size</item>
    </style>

    <style name="ProjectionActionBar" parent="android:Theme.Material.Light">
        <item name="android:actionBarStyle">@style/customActionBar</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="colorPrimaryDark">#00ffffff</item>
    </style>

    <style name="ProAppCompatTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/customActionBar</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="colorPrimaryDark">#00ffffff</item>
    </style>

    <style name="customActionBar" parent="@style/ProjectionActionStyle">
        <item name="android:contentInsetStart">@dimen/actionbar_content_insert_start</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_back</item>
        <item name="android:windowActionBarOverlay">false</item>
    </style>

    <style name="ProjectionActionStyle" parent="android:Widget.Material.ActionBar">
        <item name="android:displayOptions">showHome|showTitle|homeAsUp</item>
        <item name="android:background">@color/leshareThemeColor</item>
        <item name="android:titleTextStyle">@style/ProjectionActionBarTitle</item>
        <item name="android:icon">@android:color/transparent</item>
    </style>

    <style name="ProjectionActionBarTitle" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textColor">@color/actionbar_title_text_color</item>
        <item name="android:textSize">@dimen/actionbar_title_text_size</item>
    </style>

    <style name="author_name_style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/leshareThemeColor</item>
        <item name="android:textSize">15sp</item>
    </style>


